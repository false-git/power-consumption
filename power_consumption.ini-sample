[routeB]
# USBドングルのパス
# mac: /dev/tty.usbserial-XXXXXXXX (XXXXXXXXは文字列)
# win: COMx (xは数字)
# linux: /dev/ttyUSBx (xは数字)
device = /dev/tty.usbserial-XXXXXXXX
# UDP受信タイムアウト(秒)
timeout = 5
# デバッグフラグ
debug = True
# Bルートの認証ID
id = <Bルートの認証ID>
# Bルートのパスワード
password = <Bルートのパスワード>

# DB URL
db_url = postgresql://<username>:<password>@<hostname>/<dbname>
